/**app.wxss**/
/* 基本配置 */
html,body{
  margin: 0;
  padding: 0;
}
::-webkit-scrollbar{
  width: 0;
}
.clearfix{
  clear: both;
}
.swiperMain{
  width: 100%;
}
.slide-image{
  width: 750rpx;
  height: 900rpx;
}
.colorGery{
  color: #aaaaaa;
}
.colorPink{
  color: #ff69af;
}
.pull-right{
  float: right;
}
.pull-left{
  float: left;
}

/* 产品列表 */
.everyDayExcellent{
  display: block;
  color: grey;
  padding: 10px;
  font-size: 14px;
}
.excellentContent{
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  background-color: #e5e5e533;
}
.excellentAssembly{
  text-align: left;
  width: 356rpx;
  margin: 6rpx;
  border: 1px solid #EEEEEE;
  background-color: #FFFFFF;
  border-radius: 5px;
  /* box-shadow: 0 0 5px #101010; */
}
.excellentImage{
  width: 356rpx;
  height: 427rpx;
  border-radius: 5px;
}

.excellentChild text{
  display: block;
  font-size: 14px;
  color: grey;
}
.excellentChildFirst{
  color: #000000;
}
/* index discount allcake */

.goodsType{
  background-color: #FFFFFF;
  display: flex;
  align-items: flex-end;
}
.goodsType > view{
  width: 185rpx;
  height: 185rpx;
}
.goodsType image{
  width: 100%;
}
.goodsTitle{
  padding: 15rpx;
}
.goodsTitle image{
  width: 375rpx;
}
.goodscontent{
  padding: 27rpx;
  display: flex;
  flex-wrap: wrap;
}
.goodsItem{
  padding: 0rpx 5rpx 25rpx 5rpx;
  width: 341rpx;
}
.goodsImg{
  width: 100%;
}
.namespace{
  padding: 15rpx;
  align-content: space-between;
}
.chineseName{
  display: block;
  text-align: center;
  color: #313131;
  font-size: 16px;
  
}
.englishName{
  font-size: 12px;
  display: block;
  text-align: center;
  color: #989898;
}
.priceBuy{
  margin-top: 15rpx;
  display: flex;
  justify-content: space-between;
  align-items: baseline;
  color: #e70012;
}
.priceBuy icon{
  font-size: 20px;
}

/* 数字加减 */
.numButton{
  display: inline-block;
  width: 250rpx;
  height: 34px;
  border: 1px solid gery;
  border-radius: 5px;
  white-space: nowrap;
}

.numButton view:nth-child(2){
  border-left: 0px solid #BBB;
  border-right: 0px solid #BBB;
  background-color: #FFFFFF;
}
.numButton view{
  display: inline-block;
  width: 33.33%;
  height: 100%;
  border: 1px solid #BBB;
  background-color: #FFFFFF;
}
.numAdd,.numDel{
  color: #CACACA;
  background-color: #FFFFFF;
}
.numAdd,.numText,.numDel{
  font-size: 18px;
  text-align: center;
  line-height: 1.9;
}

/*加入购物车  */
.joinCar{
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 49px;
  background-color: #FFFFFF;
  border-top: 1px solid #EEEEEE;
}
.joinCar button{
  height: 100%;
  float: left;
  border-radius: 0px;
  font-size: 16px;
  line-height: 45px;
  border: 0px solid #FFFFFF;
}
.joinCar button::after{
  border-radius: 0px;
  border: none;
}
.joinCar button:first-child{
  width: 120rpx;
  background-color: #FFFFFF;
}
.joinCar button:nth-child(2){
  width: 315rpx;
  background-color: #f6a012;
  color: #FFFFFF;
}
.joinCar button:last-child{
  width: 315rpx;
  background-color: #ca0c16;
  color: #FFFFFF;
}

/*信件抬头  */
.letter{
  height: 8px;
  background-size: 100%;
  background-position: center;
  background-image: url('http://www.chaorenbox.com/canFly/letter.png');
}

/* 付款 */
.payMent{
  position: fixed;
  bottom: 0;
  height: 40px;
  line-height: 40px;
  color: #ec4849;
  font-size: 16px;
  width: 100%;
  display: block;
  background-color: #FFFFFF;
  border-top: 1px solid #EEEEEE;
  z-index: 9999;
}
.payMentText{
  margin-left: 20px;  
  font-size: 14px;
  color: #726964;
}
.payMentBtn{
  float: right;
  width: 80px;
  text-align: center;
  height: 40px;
  line-height: 40px;
  background-color: #ec4849;
  color: #FFFFFF;
  font-size: 14px;
  border-radius: 0px;
}
.boxShadow{
  position: fixed;
  width: 100%;
  height: 3px;
  background-color: #000000;
  bottom: -3px;
  box-shadow: 0px -1px 10px #000000;
}

.callmeBB{
  font-size: 12px;
  display: inline;
  margin: 0;
  padding: 0;
  border: none;
  line-height: 16px;
  background-color: initial;
}
.callmeBB::after{
  border: none;
}
.ft{
  background-color: #e5e5e533;
}
.ft-links{
  display: flex;
  justify-content: space-around;
  font-size: 12px;
  padding: 30px 15px 10px;
}
.ft-copyright{
  padding: 20px 0 20px;
  margin: 0 15px;
  text-align: center;
}
.ft-copyright image{
  width: 200rpx;
  height: 104rpx;
}

/* 在线客服 */
.onlineService{
  background-image: url('http://cake.chaorenlaila.com/MiniAppImg/parts/onlineService.png?v=123');
  background-size: 100% 100%;
  position: fixed;
  right: 0;
  top: 35%;
  z-index: 9;
  width: 62rpx;
  height: 237rpx;
}

/* 幕布 */
html,
page,
.curtain{
  height: 100%;
}
.curtain{
  position: fixed;
  background-color: #000000c4;
  top: 0;
  /* opacity: 0.7; */
  width: 100%;
  z-index: 99998;
}
.modal{
  background-color: #FFFFFF;
  position: fixed;
  bottom: 0;
  z-index: 99999;
  width: 100%;
}
.modalHead{
  border-bottom: 1px solid #EEEEEE;
  font-size: 16px;
  padding: 0 15px;
  display: flex;
  font-size: 14px;
  padding: 15rpx;
}
.modalHead image{
  width: 120rpx;
}
.modalGoodsName{
  padding-left: 20rpx;
  padding-right: 20rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 100%;
}
.modalGoodsName text{
  display: block;
  line-height: 30px;
}
.modalGoodsName text:last-child{
  color: #e70012;
}
.modalGoodsName image{
  margin-top: 15rpx;
  align-self: flex-start;
}
.modalContentTitle{
  color: #000000;
}
.modalContent{
  color: #999999;
  font-size: 12px;
  line-height: 1.6;
  padding: 15px;
}
.modalContent > text{
  display: block;
  padding-bottom: 10px;
}

/*地址默认按钮  */
.addressDefault{
  padding: 1rpx 7rpx 1rpx 7rpx;
  font-size: 12px;
  color: #ff63a1;
  border: 1px solid #ff63a1;
}

