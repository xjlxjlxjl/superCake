/**app.wxss**/
html,body{
  margin: 0;
  padding: 0;
}
::-webkit-scrollbar{
  width: 0;
}
.clearfix{
  clear: both;
}

.swiperMain{
  width: 100%;
  min-height: 430px;
}
.slide-image{
  width: 750rpx;
  height: 900rpx;
}

/*粉色字体  */
.colorGery{
  color: #aaaaaa;
}
.colorPink{
  color: #ff69af;
}
.pull-right{
  float: right;
}
.pull-left{
  float: left;
}

/* 蛋糕 */

/* 每日优选 */
.everyDayExcellent{
  display: block;
  background-color: #e5e5e533;
  color: grey;
  padding: 10px;
  font-size: 14px;
}
.excellentContent{
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
}
.excellentAssembly{
  text-align: center;
  width: 375rpx;
}
.excellentImage{
  width: 100%;
  height: 150px;
}
.excellentChild{
  padding: 10px;
}
.excellentChild text{
  display: block;
  font-size: 14px;
  color: grey;
}
.excellentChildFirst{
  color: #000000;
}

/* 数字加减 */
.numButton{
  display: inline-block;
  width: 250rpx;
  height: 34px;
  border: 1px solid gery;
  white-space: nowrap;
}

.numButton view:nth-child(2){
  border-left: 0px solid #BBB;
  border-right: 0px solid #BBB;
  background-color: #FFFFFF;
}
.numButton view{
  display: inline-block;
  width: 33.33%;
  height: 100%;
  border: 1px solid #BBB;
  background-color: #FFFFFF;
}
.numAdd,.numDel{
  color: #CACACA;
  background-color: #FFFFFF;
}
.numAdd,.numText,.numDel{
  font-size: 18px;
  text-align: center;
  line-height: 1.9;
}

/*信件抬头  */
.letter{
  height: 8px;
  background-size: 100%;
  background-position: center;
}

/*加入购物车  */
.joinCar{
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 49px;
  background-color: #FFFFFF;
  border-top: 1px solid #EEEEEE;
}
.joinCar button{
  height: 100%;
  float: left;
  border-radius: 0px;
  font-size: 16px;
  line-height: 45px;
  border: 0px solid #FFFFFF;
}
.joinCar button::after{
  border-radius: 0px;
}
.joinCar button:first-child{
  width: 125rpx;
  background-color: #FFFFFF;
}
.joinCar button:nth-child(2){
  width: 125rpx;
  background-color: #FFFFFF;
}
.joinCar button:nth-child(3){
  width: 250rpx;
  background-color: #f6a012;
  color: #FFFFFF;
}
.joinCar button:last-child{
  width: 250rpx;
  background-color: #ca0c16;
  color: #FFFFFF;
}

/* 付款 */
.payMent{
  position: fixed;
  bottom: 0;
  height: 40px;
  line-height: 40px;
  color: #ec4849;
  font-size: 16px;
  width: 100%;
  display: block;
  background-color: #FFFFFF;
  border-top: 1px solid #EEEEEE;
  z-index: 9999;
}
.payMentText{
  margin-left: 20px;  
  font-size: 14px;
  color: #726964;
}
.payMentBtn{
  float: right;
  width: 80px;
  text-align: center;
  height: 40px;
  line-height: 40px;
  background-color: #ec4849;
  color: #FFFFFF;
  font-size: 14px;
  border-radius: 0px;
}
.boxShadow{
  position: fixed;
  width: 100%;
  height: 3px;
  background-color: #000000;
  bottom: -3px;
  box-shadow: 0px -1px 10px #000000;
}

/*地址默认按钮  */
.addressDefault{
  padding: 1rpx 7rpx 1rpx 7rpx;
  font-size: 12px;
  color: #ff63a1;
  border: 1px solid #ff63a1;
}

